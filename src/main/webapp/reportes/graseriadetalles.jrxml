	<parameter name="p_Graseria" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
  i.descripcion,
  gci.cantidad,
  gci.precio
FROM
  graseriacosto AS gc
  JOIN graseriacostoinsumo AS gci
    ON gc.id=gci.graseriacosto_id
  JOIN insumo AS i
    ON gci.insumo_id=i.id
WHERE gc.id=$P{p_Graseria}
      AND gc.fechabaja IS NULL]]>
	</queryString>
