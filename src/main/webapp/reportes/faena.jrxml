	<queryString>
		<![CDATA[SELECT
d.numeroTropa,
' - ' AS categoria,
' - ' AS cantidad,
' - ' AS pesoizq,
' - ' AS pesoder
FROM
faenaproductor AS fp
JOIN faenacabezera AS fc
ON fp.id=fc.faenaproductor_id
JOIN dte AS d
ON fp.tropa_id=d.id
WHERE fp.faena_id= $P{p_Id}
AND fp.fechabaja IS NULL]]>
